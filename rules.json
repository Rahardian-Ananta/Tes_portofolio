{
  "rules": [
    {
      "id": "komikcast",
      "name": "Komikcast",
      "http": {
        "user_agent": "Mozilla/5.0 (Linux; Android 10; SM-G973F) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.120 Mobile Safari/537.36",
        "referer": "https://komikcast03.com/"
      },
      "list": {
        "url": "https://komikcast03.com/daftar-komik/page/{page}/?sortby=update",
        "container": "<div class=\"list-update_item\">[\\s\\S]*?</div>\\s*</div>\\s*</div>\\s*</a>\\s*</div>",
        "title": "<h3 class=\"title\"[^>]*>(.*?)</h3>",
        "link": "<a href=\"([^\"]+)\"",
        "thumb": "<img[^>]+src=\"([^\"]+)\""
      },
      "detail": {
        "judul": "<h1[^>]*class=\"komik_info-content-body-title\"[^>]*>(.*?)</h1>",
        "rating": "data-ratingkomik=\"([0-9.]+)\"",
        "author": "<b>Author:</b>\\s*([\\s\\S]*?)\\s*</span>",
        "status": "<b>Status:</b>\\s*(.*?)\\s*</span>",
        "type": "<b>Type:</b>\\s*.*?>(.*?)</a>",
        "tahun": "<b>Released:</b>\\s*([\\s\\S]*?)\\s*</span>",
        "thumbnail": "<div class=\"komik_info-body\">.*?<img[^>]+src=\"([^\"]+)\"",
        "sinopsis": "<div class=\"komik_info-description-sinopsis\"[^>]*>(.*?)</div>",
        "genre_block": "<span class=\"komik_info-content-genre\">([\\s\\S]*?)</span>",
        "genre_item": "rel=\"tag\"[^>]*>(.*?)</a>",
        "chapter_block": "<ul[^>]*id=\"chapter-wrapper\"[^>]*>(.*?)</ul>",
        "chapter_item": "<li[^>]*class=\"komik_info-chapters-item\"[^>]*>(.*?)</li>",
        "chapter_name": "<a[^>]*class=\"chapter-link-item\"[^>]*>(.*?)</a>",
        "chapter_link": "<a[^>]*href=\"([^\"]+)\"",
        "chapter_date": "<div[^>]*class=\"chapter-link-time\"[^>]*>(.*?)</div>"
      },
      "chapter": {
        "area": "<div class=\"main-reading-area\">[\\s\\S]*?</div>",
        "image": "<img[^>]+src=\"([^\"]+)\""
      }
    },
    {
      "id": "noromax",
      "name": "Noromax",
      "http": {
        "user_agent": "Mozilla/5.0",
        "referer": "https://noromax01.my.id/"
      },
      "list": {
        "url": "https://noromax01.my.id/manga/?page={page}&order=update",
        "item": "<div class=\"bsx\">.*?</a>",
        "title": "title=\"(.*?)\"",
        "link": "href=\"(.*?)\"",
        "thumb": "src=\"(.*?)\""
      },
      "detail": {
        "judul": "<h1 class=\"entry-title\".*?>(.*?)</h1>",
        "rating": "itemprop=\"ratingValue\" content=\"(.*?)\"",
        "author": "<b>Author</b>\\s*<span>\\s*(.*?)\\s*</span>",
        "status": "Status <i>(.*?)</i>",
        "type": "Type <a.*?>\\s*(.*?)\\s*</a>",
        "tahun": "<b>Released</b>\\s*<span>\\s*(.*?)\\s*</span>",
        "thumbnail": "<div class=\"thumb\".*?src=\"(.*?)\"",
        "genre_block": "<span class=\"mgen\">(.*?)</span>",
        "genre_item": "rel=\"tag\"[^>]*>(.*?)</a>",
        "sinopsis": "itemprop=\"description\">(.*?)</div>",
        "chapter_block": "<div class=\"eplister\" id=\"chapterlist\">.*?</ul>",
        "chapter_item": "<li data-num=.*?</li>",
        "chapter_name": "<span class=\"chapternum\">(.*?)</span>",
        "chapter_link": "href=\"(.*?)\"",
        "chapter_date": "<span class=\"chapterdate\">(.*?)</span>"
      },
      "chapter": {
        "image": "(?:src|data-src)=\"(https://blogger\\.googleusercontent\\.com[^\"]+)\"",
        "index": "data-index=\"(\\d+)\""
      }
    }
  ]
}
